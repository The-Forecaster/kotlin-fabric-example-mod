{
    "schemaVersion": 1,
    "id": "modid",
    "version": "${version}",
  
    "name": "Example Mod",
    "description": "This is an example description! Tell everyone what your mod is about!",
    "authors": [
      "Me!"
    ],
    "contact": {
      "homepage": "https://github.com/The-Forecaster",
      "sources": "https://github.com/The-Forecaster/kotlin-example-mod"
    },
  
    "license": "CC0-1.0",
    "icon": "assets/modid/icon.png",
  
    "environment": "*",
    "entrypoints": {
      "main": [
        "com.example.modid.ExampleMod"
      ]
    },
    "mixins": [
      "modid.mixins.json"
    ],
  
    "depends": {
      "fabricloader": ">=0.14.11",
      "fabric-api": "*",
      "minecraft": "${mcversion}",
      "java": ">=17"
    },
    "suggests": {
      "another-mod": "*"
    }
  }